{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "freshfarmpicking-react": {
      "appLocation": "src\\web",
      "apiLocation": "src\\api",
      "outputLocation": "dist",
      "apiLanguage": "node",
      "apiVersion": "18",
      "appBuildCommand": "yarn run build",
      "apiBuildCommand": "npm run build --if-present",
      "run": "yarn run dev",
      "appDevserverUrl": "http://localhost:3000",
      "apiDevserverUrl": "http://127.0.0.1:7071"
    }
  }
}